{
    "name": "FitzHugh-Nagumo",
    "description": "A simplified model of neuronal activity.",
    "parameters": {
      "a": { "initial": 0.7, "min": -1.0, "max": 1.0 },
      "b": { "initial": 0.8, "min": 0.0, "max": 1.0 },
      "c": { "initial": 0.08, "min": 0.0, "max": 0.2 }
    },
    "equations": {
      "dx_dt": "x - (x*x*x / 3.0) - y",
      "dy_dt": "c * (x + a - b*y)"
    },
    "config": {
        "velocity_scale": 10.0,
        "smoothing_factor": 0.1
    }
} 