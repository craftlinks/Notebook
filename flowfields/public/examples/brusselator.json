{
    "name": "Brusselator",
    "description": "A model for a type of autocatalytic reaction.",
    "parameters": {
      "a": { "initial": 1.0, "min": 0.0, "max": 2.0 },
      "b": { "initial": 1.7, "min": 0.0, "max": 4.0 }
    },
    "variables": {
        "X": "x + 2.0",
        "Y": "y + 2.0"
    },
    "equations": {
      "dx_dt": "a + X*X*Y - b*X - X",
      "dy_dt": "b*X - X*X*Y"
    },
    "config": {
      "velocity_scale": 1.0,
      "smoothing_factor": 0.1
    }
} 